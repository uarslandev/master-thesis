@article{gabajovaDESIGNINGVIRTUALWORKPLACE2021,
  title = {{{DESIGNING VIRTUAL WORKPLACE USING UNITY 3D GAME ENGINE}}},
  author = {Gabajov√°, Gabriela and Krajƒçoviƒç, Martin and Matys, Mari√°n and Furmannov√°, Be√°ta and Burganov√°, Nat√°lia},
  date = {2021-03-31},
